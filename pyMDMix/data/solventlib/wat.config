[GENERAL]
# solvation name (ex: ION)
name = WAT 
info = Water box
# path to off file
objectfile = WATBOX.off 
# Inside object file, name of the unit(s) of the solvent molecule alone (ex: COO, CN3)
watermodel = TIP3P 
# Name of the solvation box  unit in object file(ex: IONWAT20)
boxunit = TIP3PBOX 

[PROBES]
# map probe names with residue@atoms (ie. NEG=COO@O1,O2)
# probe names must be unique
WAT=WAT@O

[TYPES]
WAT=Wat